{
    "manifest_version": 2,
    "name": "Meet Scrape",
    "version": "0.1",
    "permissions": ["http://*/", "notifications"],
    "background": {
        "scripts": ["background.js"],
        "persistent": false 
    },
    "browser_action": {},
    "content_scripts": [
        {
          "matches": [
              "<all_urls>"  
          ],
          "js": ["content.js"]
        }
    ]
}